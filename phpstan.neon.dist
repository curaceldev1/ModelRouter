includes:
    - vendor/larastan/larastan/extension.neon

parameters:
    level: 5
    paths:
        - src
        - config
        - database
    excludePaths:
        - src/Nova/*
    configDirectories:
        - config
    reportUnmatchedIgnoredErrors: false
    ignoreErrors:
        - '#Access to an undefined property Curacel\\LlmOrchestrator\\Models\\ProcessMapping::\$client#'
        - '#Access to an undefined property Curacel\\LlmOrchestrator\\Models\\ProcessMapping::\$model#'

